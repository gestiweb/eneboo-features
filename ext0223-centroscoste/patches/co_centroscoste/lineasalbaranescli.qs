
/** @class_declaration centrosCoste */
//////////////////////////////////////////////////////////////////
// CENTROS COSTE ////////////////////////////////////////////////////////////////
class centrosCoste extends oficial /** %from: oficial */ {
	function centrosCoste( context ) { oficial( context ); }
    function init() { this.ctx.centrosCoste_init(); }
}
// CENTROS COSTE ////////////////////////////////////////////////////////////////
//////////////////////////////////////////////////////////////////

/** @class_definition centrosCoste */
/////////////////////////////////////////////////////////////////
// CENTROS COSTE ////////////////////////////////////////////////////////////////
function centrosCoste_init()
{
	this.iface.__init();
	var cursor:FLSqlCursor = this.cursor();

	if (cursor.modeAccess() == cursor.Insert) {
		var curF:FLSqlCursor = cursor.cursorRelation();
		cursor.setValueBuffer("codcentro", curF.valueBuffer("codcentro"));
		cursor.setValueBuffer("codsubcentro", curF.valueBuffer("codsubcentro"));
	}
}
// CENTROS COSTE ////////////////////////////////////////////////////////////////
/////////////////////////////////////////////////////////////////

