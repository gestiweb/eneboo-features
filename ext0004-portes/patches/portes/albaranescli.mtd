      <alias>QT_TRANSLATE_NOOP("MetaData","I.V.A.")</alias>
      <null>true</null>
      <pk>false</pk>
      <type>string</type>
      <length>10</length>
      <relation>
        <table>impuestos</table>
        <field>codimpuesto</field>
        <card>M1</card>
      </relation>
    </field>
  </xupdate:insert-after>
  <xupdate:insert-after select="/TMD[TMD,1]/field[codimpuestoportes]">
